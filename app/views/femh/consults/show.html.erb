
月份:
<%=     @month_string  %>  ,
起始日:
<%=     @month_start  %>   ,
結束日:
<%=     @month_end  %>


<h2><%= @month_string %> 月份門診人數總表_by科別_by醫師</h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>門診科別</th>
      <th>醫師名稱</th>
      <th>看診人數</th>
      <th>整月總診數</th>
      <th>平均每診人數</th>
    </tr>
  </thead>
  <tbody>
    <% @consult_all_month.each_with_index do |consult,index| %>
    <tr>
      <td> <%= index+1 %> </td>
     
      <td> <%= consult[0][0] %> </td>
      <td> <%= consult[0][1] %></td>
      <td> <%= consult[1] %> </td>
      <td> <%= render_count_open_total(consult)  %> </td>
      <td> <%= render_count_average_patient(consult)  %> </td>
   </tr>
    <% end %>
  </tbody>
</table>